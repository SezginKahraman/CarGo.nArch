{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CacheSettings": {
    "SlidingExpiration": 2
  },
  "SerilogLogConfigurations": {
    "FileLogConfiguration": {
      "FolderPath": "/logs/"
    },
    "MsSqlConfiguration": {
      "AutoCreateSqlTable": true,
      "ConnectionString": "Server=(localdb)\\MSSQLLocalDB; Database=CarGoDb; Trusted_Connection=true",
      "TableName": "Logs"
    }
  },
  "ConnectionStrings": { // win 10 authentication
    "Cargo": "Server=(localdb)\\MSSQLLocalDB; Database=CarGoDb; Trusted_Connection=true"
  },
  "AllowedHosts": "*"
}
